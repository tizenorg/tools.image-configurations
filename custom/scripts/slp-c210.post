MAJOR="2"
MINOR="0"
cat >/etc/info.ini <<EOF
[Version]
Major=$MAJOR;
Minor=$MINOR;
Build=GT-I8800_c210_`date +%Y_%m_%d`_1;
Order=0a;
[Build]
Date=`date +%Y.%m.%d`;
Time=`date +%H:%M:%S`;
EOF

ln -sf /etc/info.ini /opt/etc/info.ini
ln -sf /etc/info.ini /usr/etc/info.ini

if [ -d /opt/system/csc ]; then
	mv -f /opt/system/csc /mnt
else
	mkdir -p /mnt/csc
fi

ln -sf /mnt/csc /opt/system/csc
